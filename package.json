{
  "name": "creative_toolbox",
  "version": "1.0.0",
  "description": "A single page toolbox for creative usage",
  "main": "index.js",
  "repository": "https://github.com/SeventhDisaster/creative_toolbox.git",
  "author": "Krister <37347340+SeventhDisaster@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.23.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "webpack": "^4.42.0"
  },
  "devDependencies": {
    "webpack-dev-server": "3.10.3",
    "webpack-cli": "3.3.11",
    "webpack": "4.42.0",
    "babel-loader": "8.0.6",
    "babel-jest": "25.1.0",
    "enzyme": "3.10.0", 
    "enzyme-adapter-react-16": "1.15.1",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.6",
    "jest": "24.9.0",
    "jsdom": "15.2.1", 
    "react-addons-test-utils": "^15.6.2", 
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "@babel/preset-react": "7.8.3"
  },
  "babel": {
    "plugins": [
        "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
        "@babel/env", 
        "@babel/react"
    ]
  },
  "engines": {
    "node": "^12.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
        "src/**.(js|jsx)"
    ], 
    "setupFilesAfterEnv": [
        "<rootDir>/tests/jest-setup.js"
    ], 
    "testEnvironment": "node", 
    "testRegex": "tests/.*-test\\.(js|jsx)$"
  }, 
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --open --mode development",
    "test": "jest --coverage",
    "start": "node src/server/server.js",
    "run": "yarn build && yarn start"
  }
}
